{
    "name": "assoconnect/phpstan-rules",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/assoconnect/phpstan-rules/"
        }
    ],
    "type": "library",
    "description": "",
    "keywords": ["php"],
    "homepage": "https://github.com/assoconnect/phpstan-rules/",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AssoConnect\\PHPStanRules\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AssoConnect\\PHPStanRules\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "phpstan/phpstan": "^1.2",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpstan/phpstan-strict-rules": "^1.1",
        "roave/no-floaters": "^1.5",
        "phpstan/phpstan-webmozart-assert": "^1.2",
        "ekino/phpstan-banned-code": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2",
        "squizlabs/php_codesniffer": "^3"
    }
}
